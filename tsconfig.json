{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "jsx": "preserve",
    "types": [
      "astro/client"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "public",
    "src/assets/**/*.js"
  ]
}